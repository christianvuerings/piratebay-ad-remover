{
  "name": "Pirate Bay Ad Remover",
  "short_name": "PB Ad Remover",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Removes the ads from thepiratebay.org. The Pirate Bay is a website that indexes torrents.",
  "content_scripts": [
    {
      "js": ["js/piratebay-ad-remover.js"],
      "matches": ["http://*.thepiratebay.ac/*", "https://*.thepiratebay.ac/*",
        "http://*.thepiratebay.pe/*", "https://*.thepiratebay.pe/*",
        "http://*.thepiratebay.org/*","https://*.thepiratebay.org/*",
        "http://*.thepiratebay.se/*", "https://*.thepiratebay.se/*",
        "http://*.thepiratebay.gl/*", "https://*.thepiratebay.gl/*",
        "http://*.thepiratebay.is/*", "https://*.thepiratebay.is/*",
        "http://*.thepiratebay.sx/*", "https://*.thepiratebay.sx/*",
        "http://*.depiraatbaai.be/*", "https://*.depiraatbaai.be/*",
        "http://194.71.107.80/*", "https://194.71.107.80/*"]
    }
  ],
  "icons": {
    "32": "piratebay-ad-remover-32.png",
    "64": "piratebay-ad-remover-64.png",
    "128": "piratebay-ad-remover-128.png"
  },
  "permissions" : []
}
